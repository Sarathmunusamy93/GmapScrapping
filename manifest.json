{
  "name": "Gmap Scrapping",
  "description": "Demonstration or POC for Gmap Scrapping",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["notifications", "contextMenus", "tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js","contentScript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Do Scrap",
    "default_popup": "popup.html"
  },
  "icons": {}
}
